{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": 0,
    "no-unexpected-multiline": 0,
    "comma-dangle": 0,
    "object-curly-newline": 0,
    "max-len": ["error", 100],
    "arrow-parens": 0,
    "no-restricted-syntax": 0,
    "arrow-body-style": 0,
    "no-param-reassign": 0
  }
}
